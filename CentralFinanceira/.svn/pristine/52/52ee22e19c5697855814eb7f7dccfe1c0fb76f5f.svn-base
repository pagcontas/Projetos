package br.com.jsoft.centralfinanceira.modelo.enums;

/**
 *
 * @author ti05
 */
public enum CidadeOuLoja {

    LOJA(1, "Busca por Loja"),
    CIDADE(2, "Busca por Cidade");

    private CidadeOuLoja(int num, String descricao) {
        this.descricao = descricao;
    }

    String descricao;
    int num;

    public int getNum() {
        return num;
    }

    public String getDescricao() {
        return descricao;
    }

}
