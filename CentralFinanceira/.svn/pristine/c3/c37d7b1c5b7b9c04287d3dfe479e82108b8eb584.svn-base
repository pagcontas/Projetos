<ui:composition  xmlns="http://www.w3.org/1999/xhtml"
                 xmlns:h="http://java.sun.com/jsf/html"
                 xmlns:f="http://java.sun.com/jsf/core"
                 xmlns:ui="http://java.sun.com/jsf/facelets"
                 xmlns:p="http://primefaces.org/ui"
                 xmlns:x="http://xpert.com/faces">
    <h:form id="formDetailFatosRecarga" styleClass="uix-form-detail">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['fatosRecarga.qtd']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{fatosRecargaMB.entity.qtd}" styleClass="uix-content-detail"/>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['fatosRecarga.periodo']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{fatosRecargaMB.entity.periodo}" styleClass="uix-content-detail"/>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['fatosRecarga.arrecadado']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{fatosRecargaMB.entity.arrecadado}" styleClass="uix-content-detail">
                        <f:convertNumber minFractionDigits="2" maxFractionDigits="2" />
                    </h:outputText>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['fatosRecarga.unitarioconvenio']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{fatosRecargaMB.entity.unitarioconvenio}" styleClass="uix-content-detail">
                        <f:convertNumber minFractionDigits="2" maxFractionDigits="2" />
                    </h:outputText>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['fatosRecarga.unitarioloja']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{fatosRecargaMB.entity.unitarioloja}" styleClass="uix-content-detail">
                        <f:convertNumber minFractionDigits="2" maxFractionDigits="2" />
                    </h:outputText>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['fatosRecarga.loja']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{fatosRecargaMB.entity.loja}" styleClass="uix-content-detail">
                        <x:initializer/>
                    </h:outputText>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['fatosRecarga.conveniorecarga']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{fatosRecargaMB.entity.conveniorecarga}" styleClass="uix-content-detail">
                        <x:initializer/>
                    </h:outputText>
                </div>
           </div>
         </div>
        <p:separator/>
        <div class="uix-center">
            <p:commandButton type="button" value="#{xmsg['close']}" onclick="widgetFatosRecargaDetail.hide()" />
            <x:securityArea rolesAllowed="fatosRecarga.audit">
                <x:audit for="#{fatosRecargaMB.entity}"/>
            </x:securityArea>
        </div>
    </h:form>
</ui:composition>