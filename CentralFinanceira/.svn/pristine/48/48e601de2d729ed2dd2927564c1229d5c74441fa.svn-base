<ui:composition  xmlns="http://www.w3.org/1999/xhtml"
                 xmlns:h="http://java.sun.com/jsf/html"
                 xmlns:f="http://java.sun.com/jsf/core"
                 xmlns:ui="http://java.sun.com/jsf/facelets"
                 xmlns:p="http://primefaces.org/ui"
                 xmlns:x="http://xpert.com/faces">
    
  
    <h:form id="formCreateConveniop" >
        <p:fieldset legend="#{xmsg['generalData']}">
            <x:modalMessages/>

            <div class="container-fluid">
                 <div class="row">
                      <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12">
                         <h:outputLabel for="id" value="#{msg['conveniop.id']}:" />
                         <p:inputMask id="id" mask="9?999999999" placeHolder="" value="#{conveniopMB.entity.id}" styleClass="form-control" />
                     </div>
                      <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12">
                         <h:outputLabel for="nome" value="* #{msg['conveniop.nome']}:" />
                         <p:inputText id="nome" value="#{conveniopMB.entity.nome}" maxlength="50"  styleClass="form-control"  />
                     </div>
                      <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12">
                         <h:outputLabel for="tarifa:input" value="#{msg['conveniop.tarifa']}:" />
                         <x:inputNumber id="tarifa" value="#{conveniopMB.entity.tarifa}" styleClass="form-control"/>
                     </div>
                      <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12">
                         <h:outputLabel for="iss:input" value="#{msg['conveniop.iss']}:" />
                         <x:inputNumber id="iss" value="#{conveniopMB.entity.iss}" styleClass="form-control"/>
                     </div>

                 </div>
             </div>
        </p:fieldset>
        <h:outputText value="#{xmsg['requiredFieldsForm']}"/>
        <div class="uix-center">
            <x:securityArea rolesAllowed="conveniop.create">
                <p:commandButton process="@form" update="@form" action="#{conveniopMB.save}" value="#{xmsg['save']}" />
            </x:securityArea>
            <x:securityArea rolesAllowed="conveniop.audit">
                <x:audit for="#{conveniopMB.entity}"/>
            </x:securityArea>
        </div>
    </h:form>

</ui:composition>