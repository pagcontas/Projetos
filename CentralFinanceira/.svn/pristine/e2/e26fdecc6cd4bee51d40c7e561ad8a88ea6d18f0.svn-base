public enum Bancos {
    
    BB(1, "Banco do Brasil"),
    BACEN(2, "Banco Central do Brasil"),
    BASA(3, "Banco da Amazônia"),
    BNB(4,"Banco do Nordeste do Brasil"),
    BNDES(7,"Banco Nacional de Desenvolvimento Econômico e Social"),
    CEF(104,"Caixa Econômica Federal"),
    BRDE(46,"Banco Regional de Desenvolvimento do Extremo Sul"),
    BMG(318,"Banco BMG"),
    Cacique(263,"Banco Cacique"),
    ITAU(184,"Banco Itaú BBA"),
    ITAUBANK(184,"Banco ItaúBank"),
    BRADESCO(237,"Bradesco"),
    HSBC(399,"HSBC Bank Brasil");

    private Bancos(int codigo, String descricao) {
        this.codigo = codigo;
        this.descricao = descricao;
    }
    
    String descricao;
    int codigo;

    public String getDescricao() {
        return descricao;
    }

    public int getCodigo() {
        return codigo;
    }
    
