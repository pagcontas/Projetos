<ui:composition  xmlns="http://www.w3.org/1999/xhtml"
                 xmlns:h="http://java.sun.com/jsf/html"
                 xmlns:f="http://java.sun.com/jsf/core"
                 xmlns:ui="http://java.sun.com/jsf/facelets"
                 xmlns:p="http://primefaces.org/ui"
                 xmlns:x="http://xpert.com/faces">
    
  
    <h:form id="formCreateConvenioSite" >
        <p:fieldset legend="#{xmsg['generalData']}">
            <x:modalMessages/>

            <div class="container-fluid">
                 <div class="row">
                      <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12">
                         <h:outputLabel for="id" value="#{msg['convenioSite.id']}:" />
                         <p:inputMask id="id" mask="9?999999999" placeHolder="" value="#{convenioSiteMB.entity.id}" styleClass="form-control" />
                     </div>
                      <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12">
                         <h:outputLabel for="tipofloat" value="#{msg['convenioSite.tipofloat']}:" />
                         <h:selectOneMenu id="tipofloat" value="#{convenioSiteMB.entity.tipofloat}" converter="entityConverter" styleClass="form-control" >
                             <x:initializer/>
                             <f:selectItem itemLabel="#{xmsg['select']}" />
                             <f:selectItems value="#{findAllBean.get(classMB.tipoFloat)}" 
                                            var="tipoFloat"
                                            itemLabel="#{tipoFloat}"/>
                         </h:selectOneMenu>
                     </div>
                      <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12">
                         <h:outputLabel for="nome" value="* #{msg['convenioSite.nome']}:" />
                         <p:inputText id="nome" value="#{convenioSiteMB.entity.nome}" maxlength="50"  styleClass="form-control"  />
                     </div>
                      <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12">
                         <div class="checkbox">
                             <label>
                                 <h:selectBooleanCheckbox id="retemcomissao" value="#{convenioSiteMB.entity.retemcomissao}" />
                                 #{msg['convenioSite.retemcomissao']}
                             </label>
                         </div>
                     </div>
                      <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12">
                         <h:outputLabel for="tarifa:input" value="#{msg['convenioSite.tarifa']}:" />
                         <x:inputNumber id="tarifa" value="#{convenioSiteMB.entity.tarifa}" styleClass="form-control"/>
                     </div>
                      <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12">
                         <h:outputLabel for="diasfloat" value="#{msg['convenioSite.diasfloat']}:" />
                         <p:inputMask id="diasfloat" mask="9?999999999" placeHolder="" value="#{convenioSiteMB.entity.diasfloat}" styleClass="form-control" />
                     </div>
                      <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12">
                         <h:outputLabel for="iss:input" value="#{msg['convenioSite.iss']}:" />
                         <x:inputNumber id="iss" value="#{convenioSiteMB.entity.iss}" styleClass="form-control"/>
                     </div>
                      <div class="form-group col-lg-4 col-md-6 col-sm-6 col-xs-12">
                         <div class="checkbox">
                             <label>
                                 <h:selectBooleanCheckbox id="ativo" value="#{convenioSiteMB.entity.ativo}" />
                                 #{msg['convenioSite.ativo']}
                             </label>
                         </div>
                     </div>

                 </div>
             </div>
        </p:fieldset>
        <h:outputText value="#{xmsg['requiredFieldsForm']}"/>
        <div class="uix-center">
            <x:securityArea rolesAllowed="convenioSite.create">
                <p:commandButton process="@form" update="@form" action="#{convenioSiteMB.save}" value="#{xmsg['save']}" />
            </x:securityArea>
            <x:securityArea rolesAllowed="convenioSite.audit">
                <x:audit for="#{convenioSiteMB.entity}"/>
            </x:securityArea>
        </div>
    </h:form>

</ui:composition>