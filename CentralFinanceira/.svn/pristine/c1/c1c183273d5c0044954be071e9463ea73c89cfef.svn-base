<ui:composition  xmlns="http://www.w3.org/1999/xhtml"
                 xmlns:h="http://java.sun.com/jsf/html"
                 xmlns:f="http://java.sun.com/jsf/core"
                 xmlns:ui="http://java.sun.com/jsf/facelets"
                 xmlns:p="http://primefaces.org/ui"
                 xmlns:x="http://xpert.com/faces">
    <h:form id="formDetailEndereco" styleClass="uix-form-detail">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['endereco.endereco']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{enderecoMB.entity.endereco}" styleClass="uix-content-detail"/>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['endereco.bairro']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{enderecoMB.entity.bairro}" styleClass="uix-content-detail"/>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['endereco.cidade']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{enderecoMB.entity.cidade}" styleClass="uix-content-detail">
                        <x:initializer/>
                    </h:outputText>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['endereco.cep']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{enderecoMB.entity.cep}" styleClass="uix-content-detail"/>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['endereco.numero']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{enderecoMB.entity.numero}" styleClass="uix-content-detail"/>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['endereco.complemento']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{enderecoMB.entity.complemento}" styleClass="uix-content-detail"/>
                </div>
           </div>
         </div>
        <p:separator/>
        <div class="uix-center">
            <p:commandButton type="button" value="#{xmsg['close']}" onclick="PF('widgetEnderecoDetail').hide()" />
            <x:securityArea rolesAllowed="endereco.audit">
                <x:audit for="#{enderecoMB.entity}"/>
            </x:securityArea>
        </div>
    </h:form>
</ui:composition>