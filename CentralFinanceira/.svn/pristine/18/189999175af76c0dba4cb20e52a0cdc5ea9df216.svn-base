<ui:composition  xmlns="http://www.w3.org/1999/xhtml"
                 xmlns:h="http://java.sun.com/jsf/html"
                 xmlns:f="http://java.sun.com/jsf/core"
                 xmlns:ui="http://java.sun.com/jsf/facelets"
                 xmlns:p="http://primefaces.org/ui"
                 xmlns:x="http://xpert.com/faces">
    <h:form id="formDetailLoja" styleClass="uix-form-detail">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['loja.nome']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{lojaMB.entity.nome}" styleClass="uix-content-detail"/>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['loja.razaosocial']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{lojaMB.entity.razaosocial}" styleClass="uix-content-detail"/>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['loja.cnpj']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{lojaMB.entity.cnpj}" styleClass="uix-content-detail"/>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="Inscrição Estadual:" styleClass="control-label" /><br/>
                    <h:outputText value="#{lojaMB.entity.inscricaoEstadual}" styleClass="uix-content-detail"/>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['loja.cidade']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{lojaMB.entity.cidade}" styleClass="uix-content-detail">
                        <x:initializer/>
                    </h:outputText>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['loja.segmento']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{lojaMB.entity.segmento}" styleClass="uix-content-detail">
                        <x:initializer/>
                    </h:outputText>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['loja.endereco']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{lojaMB.entity.endereco}" styleClass="uix-content-detail"/>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['loja.bairro']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{lojaMB.entity.bairro}" styleClass="uix-content-detail"/>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['loja.cep']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{lojaMB.entity.cep}" styleClass="uix-content-detail"/>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                    <h:outputLabel value="#{msg['loja.ativo']}:" styleClass="control-label" /><br/>
                    <h:outputText value="#{lojaMB.entity.ativo}" converter ="yesNoConverter"  styleClass="uix-content-detail"/>
                </div>
           </div>
         </div>
        <p:separator/>
        <div class="uix-center">
            <p:commandButton type="button" value="#{xmsg['close']}" onclick="PF('widgetLojaDetail').hide()" />
            <x:securityArea rolesAllowed="loja.audit">
                <x:audit for="#{lojaMB.entity}"/>
            </x:securityArea>
        </div>
    </h:form>
</ui:composition>